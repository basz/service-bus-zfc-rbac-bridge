    "repositories": [
        {
            "type": "git",
            "url": "git@github.com:basz/zfc-rbac.git"
        }
    ],
    "require": {
        "php": "~5.5|~7.0",
        "prooph/service-bus": ">=4.3 <6.0",
        "zf-commons/zfc-rbac": "dev-WIP-remove-identity-provider as 3.0 || ^2.5.2 || ^3.0"
    },
    "require-dev": {
        "zendframework/zend-authentication": "~2.5",
        "phpunit/phpunit": "~4.8",
        "friendsofphp/php-cs-fixer": "1.7.*",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Prooph\\ServiceBusZfcRbacBridge\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ProophTest\\ServiceBusZfcRbacBridge\\": "tests/"
        }
    }
